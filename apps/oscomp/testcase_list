# TODO
# glibc libctest测例
# 复制动态库
/glibc/busybox mkdir -p /lib
/glibc/busybox cp /glibc/lib/dlopen_dso.so /lib/
/glibc/busybox cp /glibc/lib/ld-linux-riscv64-lp64d.so.1 /lib/
/glibc/busybox cp /glibc/lib/libc.so /lib/
/glibc/busybox cp /glibc/lib/libm.so /lib/
/glibc/busybox cp /glibc/lib/tls_align_dso.so /lib/
/glibc/busybox cp /glibc/lib/tls_get_new-dtv_dso.so /lib/
/glibc/busybox cp /glibc/lib/tls_init_dso.so /lib/

/glibc/busybox echo "#### OS COMP TEST GROUP START libctest-glibc ####"
/glibc/busybox sh /glibc/run-static.sh
/glibc/busybox sh /glibc/run-dynamic.sh
/glibc/busybox echo "#### OS COMP TEST GROUP END libctest-glibc ####"

# musl libctest测例
# 复制动态库
/musl/busybox mkdir -p /lib
/musl/busybox cp /musl/lib/dlopen_dso.so /lib/
/musl/busybox cp /musl/lib/libc.so /lib/
/musl/busybox cp /musl/lib/tls_align_dso.so /lib/
/musl/busybox cp /musl/lib/tls_get_new-dtv_dso.so /lib/
/musl/busybox cp /musl/lib/tls_init_dso.so /lib/

/musl/busybox echo "#### OS COMP TEST GROUP START libctest-musl ####"
/musl/busybox sh /musl/run-static.sh
/musl/busybox sh /musl/run-dynamic.sh
/musl/busybox echo "#### OS COMP TEST GROUP END libctest-musl ####"


